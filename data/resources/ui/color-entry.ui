<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="ColorEntry" parent="GtkEntry">
    <signal name="icon-press" handler="icon_pressed" swapped="true" />
    <property name="halign">center</property>
    <property name="valign">center</property>
    <property name="secondary-icon-name">color-select-symbolic</property>
    <property name="secondary-icon-activatable">True</property>
    <style>
      <class name="monospace" />
    </style>
  </template>
</interface>