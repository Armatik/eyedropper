<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/com/github/finefindus/eyedropper/" id="@app-id@" gettext-domain="@gettext-package@">
    <key name="window-width" type="i">
      <default>300</default>
      <summary>Window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>425</default>
      <summary>Window height</summary>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Window maximized state</summary>
    </key>
    <key name="format-order" type="as">
      <default>[ 'name', 'hex', 'rgb', 'hsl', 'hsv', 'cmyk', 'xyz', 'cielab', 'hwb', 'hcl', 'lms', 'hunterlab' ]</default>
      <summary>Format Order</summary>
      <description>Order, in which the available formats are displayed.</description>
    </key>
    <key name="alpha-position" type="i">
      <default>0</default>
      <summary>Alpha Value Position</summary>
      <description>At which position the alpha values are shown, e.g. not shown, the end or at the start. Thsi preference is used in multiple formats, like hex, rgb and hsl.</description>
    </key>
    <key name="show-hex-model" type="b">
      <default>true</default>
      <summary>Show hex format</summary>
      <description>Changes the visibility of the hex format.</description>
    </key>
    <key name="show-rgb-model" type="b">
      <default>true</default>
      <summary>Show RGB format</summary>
      <description>Changes the visibility of the RGB format.</description>
    </key>
    <key name="show-hsl-model" type="b">
      <default>true</default>
      <summary>Show HSL format</summary>
      <description>Changes the visibility of the HSL format.</description>
    </key>
    <key name="show-hsv-model" type="b">
      <default>true</default>
      <summary>Show HSV format</summary>
      <description>Changes the visibility of the HSV format.</description>
    </key>
    <key name="show-cmyk-model" type="b">
      <default>true</default>
      <summary>Show CMYK format</summary>
      <description>Changes the visibility of the CMYK format.</description>
    </key>
    <key name="show-xyz-model" type="b">
      <default>false</default>
      <summary>Show XYZ format</summary>
      <description>Changes the visibility of the XYZ format.</description>
    </key>
    <key name="show-cie-lab-model" type="b">
      <default>false</default>
      <summary>Show CIELAB format</summary>
      <description>Changes the visibility of the CIELAB format.</description>
    </key>
    <key name="show-hwb-model" type="b">
      <default>false</default>
      <summary>Show HWB format</summary>
      <description>Changes the visibility of the HWB format.</description>
    </key>
    <key name="show-hcl-model" type="b">
      <default>false</default>
      <summary>Show HCL format</summary>
      <description>Changes the visibility of the HCL format.</description>
    </key>
    <key name="show-color-name" type="b">
      <default>false</default>
      <summary>Show color name</summary>
      <description>Changes the visibility of the color name.</description>
    </key>
    <key name="show-lms-format" type="b">
      <default>false</default>
      <summary>Show LMS format</summary>
      <description>Changes the visibility of the LMS format.</description>
    </key>
    <key name="show-hunter-lab-format" type="b">
      <default>false</default>
      <summary>Show Hunter Lab</summary>
      <description>Changes the visibility of the Hunter Lab format.</description>
    </key>
    <key name="name-source-basic" type="b">
      <default>true</default>
      <summary>Show basic color names</summary>
      <description>Show color names from the w3c basic keyword set.</description>
    </key>
    <key name="name-source-extended" type="b">
      <default>true</default>
      <summary>Show extended color names</summary>
      <description>Show color names from the w3c extended keyword set.</description>
    </key>
    <key name="name-source-xkcd" type="b">
      <default>false</default>
      <summary>Show xkcd color names</summary>
      <description>Show color names from the xkcd color survey.</description>
    </key>
    <key name="name-source-gnome-palette" type="b">
      <default>true</default>
      <summary>Show GNOME palette color names</summary>
      <description>Show color names from the GNOME color palette.</description>
    </key>
    <key name="cie-illuminants" type="i">
      <default>5</default>
      <summary>Color Illuminant</summary>
      <description>Used XYZ Tristimulus values as the CIE illuminant</description>
    </key>
    <key name="cie-standard-observer" type="i">
      <default>0</default>
      <summary>CIE standard observer</summary>
      <description>If 2째 or 10째 values are used for the CIE standard observer. Can only be 0 (2째) or 1 (10째).</description>
    </key>
    <key name="use-default-precision" type="b">
      <default>true</default>
      <summary>Precision</summary>
      <description>Use the default recommended precision</description>
    </key>
    <key name="precision-digits" type="u">
      <default>2</default>
      <summary>Precision</summary>
      <description>Number of digits displayed after the numeric point</description>
    </key>
  </schema>
</schemalist>